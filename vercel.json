{
  "version":2,
  "builds":[
    {
      "src":"TrainWiseAI/main.py"
      "use":"@vercel/python",
      "config":{"maxLambdaSize":"15mb","runtime":"python3.9"}
    },
    {
      "src":"build_files.sh",
      "use":"@vercel/staticfiles-build",
      "config":{
        "distDir":"staticfiles_build"
      }
    }
  ],
  "routes":[
    {
      "src":"/static/(.*)",
      "dest":"/static/$1"
    },
    {
      "src":"/(.*)",
      "dest":"TrainWiseAI/app.py"
    }
  ]
}
